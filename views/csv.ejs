<% include header %>
    <% include navbar %>
    <!-- los sÃ­mbolos de menor y mayor con el porcentaje, se usan para poner
         javascript dentro y que se ejecute del lado del servidor -->

    <h1>CSV</h1>

    <table>
        <% csv.forEach(function(row, index) { %>
            <% if(index == 0) { %>
                <tr class="<%= row.rowClass %>">
                    <% row.value.forEach(function(cell) { %>
                        <th><%=cell %></th>
                    <% }); %>
                </tr>
            <% } else { %>
                <tr class="<%= row.rowClass %>">
                    <% row.value.forEach(function(cell) { %>
                        <td><%=cell %></td>
                    <% }); %>
                </tr>
            <% } %>
        <% }); %>
    </table>

<% include footer %>
